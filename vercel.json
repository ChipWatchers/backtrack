{
    "rewrites": [
        {
            "source": "/",
            "destination": "/ui/index.html"
        },
        {
            "source": "/styles.css",
            "destination": "/ui/styles.css"
        },
        {
            "source": "/ui.js",
            "destination": "/ui/ui.js"
        },
        {
            "source": "/app/(.*)",
            "destination": "/app/$1"
        }
    ]
}